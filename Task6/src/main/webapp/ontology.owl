@prefix :  <https://semantiweb-cottagebooking.azurewebsites.net/task6/ontology.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

@base <https://semantiweb-cottagebooking.azurewebsites.net/task6/ontology.owl> .

<https://semantiweb-cottagebooking.azurewebsites.net/task6/ontology.owl> a owl:Ontology .

#################################################################
#  Classes
#################################################################
:Cottage a rdfs:Class .
:BookingRequest a rdfs:Class . # We could collect requests and input them to the database
:Booking a rdfs:Class .

#################################################################
#  Object Properties
#################################################################
:hasCottage a rdf:Property ;
  rdfs:domain :Booking ;
  rdfs:range :Cottage .

:bookingProposals a rdf:Property ;
  rdfs:domain :BookingRequest ;
  rdfs:range :Booking . #TODO as a list

:bookings a rdf:Property ;
  rdfs:domain :BookingRequest ;
  rdfs:range :Booking . #TODO as a list

:requestedAs a rdf:Property ;
  rdfs:domain :BookingRequest ;
  rdfs:range :Booking .

#################################################################
#  Data Properties
#################################################################
:hasPlaces a rdf:Property ;
  rdfs:domain :Cottage ;
  rdfs:range xsd:nonNegativeInteger .

:hasBedrooms a rdf:Property ;
  rdfs:domain :Cottage ;
  rdfs:range xsd:nonNegativeInteger .

:hasClosestCity a rdf:Property ;
  rdfs:domain :Cottage ;
  rdfs:range xsd:string .

:hasBookerName a rdf:Property ;
  rdfs:domain :Booking ;
  rdfs:range xsd:string .

:hasImage a rdf:Property ;
  rdfs:domain :Cottage ;
  rdfs:range xsd:string .

:hasId a rdf:Property ;
  rdfs:main :Booking ;
  rdfs:range xsd:integer .

:hasDistanceFromTheLake a  rdf:Property ;
  rdfs:domain :Cottage ;
  rdfs:range xsd:float .

:hasBookingDuration a rdf:Property ;
  rdfs:domain :Booking ;
  rdfs:range xsd:nonNegativeInteger .

:hasAddress a rdf:Property ;
  rdfs:domain :Cottage ;
  rdfs:range xsd:string .

:hasDistanceToCity a rdf:Property ;
  rdfs:domain :Cottage ;
  rdfs:range xsd:float .

:hasStartDate a rdf:Property ;
  rdfs:domain :Booking ;
  rdfs:range xsd:date .

:hasName a rdf:Property ;
  rdfs:domain :Cottage ;
  rdfs:range xsd:string .
