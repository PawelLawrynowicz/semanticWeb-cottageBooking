@prefix : <http://localhost:8081/task6/BookingDB.ttl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
#################################################################
#    Classes
#################################################################

:Booking a rdfs:Class .
:Cottage a rdfs:Class .

#################################################################
#    Object Properties
#################################################################

:hasCottage a  rdf:Property ;
            rdfs:domain :Booking ;
            rdfs:range  :Cottage .

#################################################################
#    Data properties
#################################################################

#Booking properties
:hasId a rdf:Property ;
       rdfs:domain :Booking ;
       rdfs:range  xsd:integer .
:hasBookerName a rdf:Property ;
               rdfs:domain :Booking ;
               rdfs:range  xsd:string .
:hasStartDate a rdf:Property ;
              rdfs:domain :Booking ;
              rdfs:range  xsd:date .
:hasBookingDuration a rdf:Property ;
                    rdfs:domain :Booking ;
                    rdfs:range  xsd:nonNegativeInteger .
:startDayMaxShift a rdf:Property ;
                  rdfs:domain :Booking ;
                  rdfs:range  xsd:integer .

#Cottage properties
:hasName  a rdf:Property ;
          rdfs:domain :Cottage ;
          rdfs:range xsd:string .
:hasDistanceFromTheLake a rdf:Property ;
                        rdfs:domain :Cottage ;
                        rdfs:range  xsd:float .
:hasBedrooms a rdf:Property ;
             rdfs:domain :Cottage ;
             rdfs:range  xsd:nonNegativeInteger .
:hasPlaces a rdf:Property ;
           rdfs:domain :Cottage ;
           rdfs:range  xsd:nonNegativeInteger .
:hasClosestCity a rdf:Property ;
                rdfs:domain :Cottage ;
                rdfs:range  xsd:string .
:hasDistanceToCity a rdf:Property ;
                   rdfs:domain :Cottage ;
                   rdfs:range  xsd:float .
:hasAddress a rdf:Property ;
            rdfs:domain :Cottage ;
            rdfs:range  xsd:string .
:hasImage a rdf:Property ;
          rdfs:domain :Cottage ;
          rdfs:range  xsd:string .

